# =============================================================================
# AIOS â€“ Environment variables sample
# Copy this file to .env and fill in your values.
# =============================================================================

# -----------------------------------------------------------------------------
# Database (required)
# -----------------------------------------------------------------------------
DATABASE_URL="postgresql://user:password@localhost:5432/aios"

# -----------------------------------------------------------------------------
# AI (required for POST /v1/expose/prompt)
# -----------------------------------------------------------------------------
AI_GATEWAY_API_KEY="your-gateway-api-key"

# Model identifier (optional; default: anthropic/claude-sonnet-4.5)
AI_MODEL="anthropic/claude-sonnet-4.5"

# -----------------------------------------------------------------------------
# Server
# -----------------------------------------------------------------------------
PORT=3000

# -----------------------------------------------------------------------------
# Swagger / API docs (optional; used for title and server list in /v1/docs)
# -----------------------------------------------------------------------------
PLATFORM_NAME="AIOS"
PLATFORM_URL="http://localhost:3000"
DEVELOPMENT_URL="localhost:3000"
PRODUCTION_URL="your-production-host.com"
