{
  "name": "aios",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=22.10.7"
  },
  "scripts": {
    "build": "pnpm prisma migrate deploy && pnpm prisma generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prisma:dev": "pnpm prisma migrate dev && pnpm prisma generate && pnpm prisma studio",
    "prisma:prod": "pnpm prisma db pull && pnpm prisma migrate deploy && pnpm prisma generate && pnpm prisma studio",
    "prisma:studio": "pnpm prisma studio",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@ai-sdk/gateway": "^3.0.49",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "@scalar/nestjs-api-reference": "^1.0.24",
    "@valyu/ai-sdk": "^1.1.0",
    "ai": "^6.0.90",
    "axios": "^1.13.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "express-rate-limit": "^8.2.1",
    "googleapis": "^171.4.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.3",
    "moment-timezone": "^0.6.0",
    "multer": "^2.0.2",
    "pg": "^8.18.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "winston-daily-rotate-file": "^5.0.0",
    "xlsx": "^0.18.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/config": "^4.0.3",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/swagger": "^11.2.6",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/nodemailer": "^7.0.10",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "handlebars": "^4.7.8",
    "jest": "^30.0.0",
    "nest-winston": "^1.10.2",
    "nodemailer": "^8.0.1",
    "prettier": "^3.4.2",
    "prisma": "^7.4.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "winston": "^3.19.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
